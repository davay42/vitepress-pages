{
	"name": "vitepress-pages",
	"version": "1.0.4",
	"keywords": [
		"vite",
		"vitepress",
		"plugin",
		"markdown",
		"pages",
		"digital garden"
	],
	"bugs": {
		"url": "https://github.com/davay42/vitepress-pages/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/davay42/vitepress-pages.git"
	},
	"funding": {
		"type": "github",
		"url": "https://github.com/sponsors/davay42"
	},
	"license": "MIT",
	"author": "davay42 <davay@defucc.me>",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./browser": {
			"import": "./dist/browser.js",
			"require": "./dist/browser.cjs"
		}
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"scripts": {
		"build": "vitepress build",
		"dev": "vitepress dev",
		"preview": "vitepress serve",
		"build-nested": "vitepress build nested",
		"dev-nested": "vitepress dev nested",
		"lib": "tsup"
	},
	"dependencies": {
		"gray-matter": "^4.0.3",
		"sharp": "^0.31.3"
	},
	"devDependencies": {
		"@types/node": "^18.14.0",
		"app-root-path": "^3.1.0",
		"tsup": "^6.6.3",
		"typescript": "4.9.5",
		"vite": "^4.1.2",
		"vite-plugin-pages": "^0.28.0",
		"vite-plugin-pages-sitemap": "^1.4.5",
		"vitepress": "1.0.0-alpha.46"
	},
	"peerDependencies": {
		"vite-plugin-pages": "^0.28.0"
	},
	"tsup": {
		"entry": [
			"src/index.ts",
			"src/browser.ts"
		],
		"format": [
			"esm",
			"cjs"
		],
		"splitting": false,
		"sourcemap": true,
		"clean": true,
		"dts": true
	}
}