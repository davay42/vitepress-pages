{
	"name": "vitepress-pages",
	"version": "2.0.0-beta.14",
	"keywords": [
		"vite",
		"vitepress",
		"plugin",
		"markdown",
		"pages",
		"digital garden"
	],
	"bugs": {
		"url": "https://github.com/davay42/vitepress-pages/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/davay42/vitepress-pages.git"
	},
	"funding": {
		"type": "github",
		"url": "https://github.com/sponsors/davay42"
	},
	"license": "MIT",
	"author": "davay42 <davay@defucc.me>",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./media": {
			"import": "./dist/media.js"
		},
		"./head": {
			"import": "./dist/head.js"
		}
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"types": "./dist/types.d.ts",
	"scripts": {
		"build": "vitepress build",
		"dev": "vitepress dev",
		"preview": "vitepress serve",
		"lib": "tsup"
	},
	"dependencies": {
		"sharp": "^0.32.1",
		"vue": "3.3.4"
	},
	"devDependencies": {
		"@types/node": "^18.16.16",
		"sitemap": "^7.1.1",
		"tsup": "^6.7.0",
		"typescript": "5.1.3",
		"vite": "4.3.9",
		"vitepress": "1.0.0-beta.1"
	},
	"peerDependencies": {
		"vitepress": "^1.0.0-beta.1"
	},
	"tsup": {
		"entry": [
			"src/media.ts",
			"src/index.ts",
			"src/head.ts"
		],
		"format": [
			"esm"
		],
		"splitting": false,
		"sourcemap": false,
		"clean": true,
		"dts": true,
		"publicDir": "types"
	}
}
