  },
  "keywords": [
    "vite",
    "vitepress",
    "plugin",
    "markdown",
    "pages",
    "digital garden"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./browser": {
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.js"
    }
  },
  "scripts": {
    "dev": "vitepress dev",
    "build": "vitepress build",
    "lib": "tsup src/index.js src/browser.js --format esm,cjs",
    "publish": "pnpm run lib; pnpm version patch; pnpm publish"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "sharp": "^0.30.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.4",
    "tsup": "^5.12.1",
    "vite": "^2.8.6",
    "vitepress": "0.22.2",
    "vite-plugin-md": "^0.12.4",
    "vite-plugin-pages": "^0.22.0",
    "vite-plugin-pages-sitemap": "^1.1.11",
    "vue": "^3.2.31"
  }
}
